  "description": "Get user data and posts by scraping Instagram's user page. Without API key or oAuth!",
  "main": "index.js",
  "author": "BART! <bart@bartweb.cz>",
  "scripts": {
    "start": "tsc -w",
    "prebuild": "rimraf dist",
