    "demo": "ts-node demo",
    "lint": "eslint ./src/**/**/* --fix",
    "test": "jest",
    "prepare": "husky install",
    "postinstall": "yarn husky install",
    "test:coverage": "jest --collect-coverage",
    "postversion": "git push && git push --follow-tags",
    "release:patch": "git checkout master && npm version patch -m \"chore(update): patch release %s ðŸ› \"",
    "release:minor": "git checkout master && npm version minor -m \"chore(update): release %s ðŸš€\"",
    "release:major": "git checkout master && npm version major -m \"chore(update): major release %s ðŸ’¥ \""
  },
  "dependencies": {
    "cross-fetch": "^3.1.4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.2",
    "@types/node": "16.9.6",
    "@types/node-fetch": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.1",
    "jest-fetch-mock": "^3.0.3",
    "npm-prepare-dist": "^0.3.1",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "repository": {
    "url": "git+https://github.com/bartholomej/instagram-scraper-api.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/bartholomej/instagram-scraper-api/issues"
  },
  "homepage": "https://github.com/bartholomej/instagram-scraper-api#readme",
  "husky": {
    "hooks": {
